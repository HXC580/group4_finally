<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper  namespace="com.example.group4.mapper.student.ex.WlanEXMapper">

    <select id="selectPwdByStuId" resultType="String">
        select pass from wlan where student_id=#{id}
    </select>

    <update id="bindPwdByStuId">
        update wlan set pass=#{pwd} where student_id=#{id}
    </update>

    <insert id="setBind">
        insert into wlan (student_id,pass)values (#{sid},#{pwd})
    </insert>

</mapper>